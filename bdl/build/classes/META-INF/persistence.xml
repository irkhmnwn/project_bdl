<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="bdlPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>presistt.Kendaraan</class>
    <class>presistt.Penyewaan</class>
    <class>presistt.Pelanggan</class>
    <class>presistt.Pegawai</class>
    <class>presistt.DetailPenyewaan</class>
    <class>presistt.Pembayaran</class>
    <class>presistt.Pengembalian</class>
    <class>presistt.Sopir</class>
    <class>presistt.BiayaDenda</class>
    <class>presistt.Perawatan</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5433/bdl"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="100604"/>
    </properties>
  </persistence-unit>
</persistence>
